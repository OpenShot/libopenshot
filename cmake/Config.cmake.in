@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

if(@PACKAGE_OpenShotAudio_FOUND@)
  find_dependency(OpenShotAudio)
endif()

if(@HAVE_RESVG@)
  list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")
  find_dependency(Resvg)
else()
  list(APPEND QT_EXTRA_COMPONENTS Svg)
endif()

find_dependency(Qt5 COMPONENTS Core Gui Widgets ${QT_EXTRA_COMPONENTS})

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_EXPORT_NAME@Targets.cmake")

check_required_components(@PROJECT_EXPORT_NAME@)
